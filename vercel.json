{
    "version": 2,
    "builds": [
      {
        "src": "backend/main.py",
        "use": "@vercel/python"
      },
      {
        "src": "frontend/package.json",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/backend/(.*)",
        "dest": "backend/main.py"
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }